# choose one provider: "openai" or "nebius"
PROVIDER=openai

# OpenAI (recommended for quick tests)
OPENAI_API_KEY=sk-...

# Nebius (optional; keep empty if not used)
NEBIUS_API_KEY=
NEBIUS_MODEL=Qwen/Qwen2.5-32B-Instruct

# LangChain (optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=

# App
DATASET_SOURCE=hf         # "hf" or "csv"
SESSION_ID=local-test     # default
